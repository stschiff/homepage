  number   =  1,
  pages    = "20939",
  month    =  dec,
  day      =  3,
  year     =  2022,
  url      = "http://dx.doi.org/10.1038/s41598-022-25384-y",
  file     = "All Papers/W/Wang et al. 2022 - 4000-year-old hair from the Middle Nile highlights un ... n pattern and a potential source of early eastern Africa pastoralists.pdf",
  language = "en",
  issn     = "2045-2322",
  pmid     = "36463384",
  doi      = "10.1038/s41598-022-25384-y",
  pmc      = "PMC9719486",
  keywords = "role\_lead"
}

